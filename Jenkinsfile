pipeline {
    agent any
    stages {
        stage ('Initialize') {
            steps {
                echo  "Initializing the Code File"
            }
        }
 
        stage ('Build') {
            steps {
                echo 'Hello World'
            }
        }
        stage ('deply'){
            steps {
                echo 'Deployed an Artifact'
            }
        }
    }
}